{
    "java-executable": "java",
    "jvm-args": [
        "-Xms2048M",
        "-Xmx2048M"
    ],
    "module-data": {
        "module_backup": {
            "prefer-7z": true,
            "fast-backup": true
        }
    },
    "hide-gui": false,
    "upgrade-all-chunks-on-version-mismatch": false,
    "server-list": [
        {
            "name": "Release",
            "version": "release",
            "description": "Latest release.",
            "auto-update": true,
            "auto-restart": true,
            "read-only": false,
            "initialize": []
        },
        {
            "name": "Private",
            "version": "snapshot",
            "description": "For private multiplayer.",
            "auto-update": false,
            "auto-restart": true,
            "read-only": false,
            "initialize": []
        },
        {
            "name": "Research",
            "version": "release",
            "description": "For research purposes.",
            "auto-update": true,
            "auto-restart": false,
            "read-only": false,
            "initialize": [
                "backup schedule 30m 4"
            ]
        },
        {
            "name": "Snapshot",
            "version": "snapshot",
            "description": "Latest snapshot.",
            "auto-update": true,
            "auto-restart": true,
            "read-only": false,
            "initialize": []
        },
        {
            "name": "Minigame",
            "version": "snapshot",
            "description": "Minigames",
            "auto-update": true,
            "auto-restart": true,
            "read-only": true,
            "initialize": []
        },
        {
            "name": "Test",
            "version": "custom",
            "description": "Test Server",
            "auto-update": false,
            "auto-restart": false,
            "read-only": false,
            "initialize": [],
            "__comment_initialize": [
                "backup schedule 1h 2",
                "notify join on",
                "notify leave on",
                "notify chat add (|.*\\s+)@[Ss]erver(|\\s+.*)$",
                "notify chat add (|.*\\s+)@[Aa]gent[Mm](|\\s+.*)$"
            ]
        }
    ]
}